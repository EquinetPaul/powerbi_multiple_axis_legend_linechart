{
    "dataRoles": [
        {
            "name": "category",
            "kind": "Grouping",
            "displayName": "Category"
        },
        {
            "name": "measure",
            "kind": "Measure",
            "displayName": "Measure"
        },
        {
            "name": "series",
            "kind": "Grouping",
            "displayName": "Series"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    },
                    "measure": {
                        "max": 2
                    },
                    "series": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "category"
                    }
                },
                "values": {
                    "group": {
                        "by": "series",
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
    "objects": {
        "colorSelector": {
            "displayName": "Data Colors",
            "properties": {
                "fill": {
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        }
    },
    "privileges": []
}